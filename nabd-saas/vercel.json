{
    "crons": [
        {
            "path": "/api/cron/cleanup-uploads",
            "schedule": "0 0 * * *"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ],
    "functions": {
        "src/app/api/nabd/route.ts": {
            "maxDuration": 60
        },
        "src/app/api/browse/route.ts": {
            "maxDuration": 15
        }
    }
}